<% include ./../partials/header %>
 
<h1>Open Tickets</h1>
 
<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Subject</th>
        </tr>
    </thead>
    <tbody>
        <ul>
            <% for(var i = 0; i < mainValues.length; i++) { %>
                <li><%= mainValues[i]['name'] %></li>
                <li><%= mainValues[i]['email'] %></li>
                <li><%= mainValues[i]['subject'] %></li>
                <li>
                    <a href="/admin/ticket/<%=  mainValues[i]['_id'] %>">Reply</a>
                </li>
            <% } %>
        </ul>
    </tbody>
</table>
<% include ./../partials/footer %>